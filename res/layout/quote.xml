<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content">
	<TextView android:id="@+id/quote_id" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="ID"/>
	<TextView android:id="@+id/quote_rank" android:layout_height="wrap_content"
		android:layout_toRightOf="@id/quote_id" android:layout_width="fill_parent" android:gravity="right"
		android:text="RANK" />
	<TextView android:id="@+id/quote_text" android:layout_below="@id/quote_id"
		android:layout_height="wrap_content" android:layout_width="wrap_content"
		android:text="TEXT" />
</RelativeLayout>
